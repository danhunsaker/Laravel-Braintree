{
    "name": "faustbrian/laravel-braintree",
    "description": "A Braintree bridge for Laravel",
    "keywords": ["laravel", "framework", "Laravel-Braintree", "Laravel Braintree", "Brian Faust", "faustbrian"],
    "license": "MIT",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@brianfaust.de",
        "homepage": "https://brianfaust.de"
    }],
    "require": {
        "php": "^7.0",
        "illuminate/support": "5.3.* || 5.4.* || 5.5.*",
        "graham-campbell/manager": "^2.4",
        "braintree/braintree_php": "^3.17"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.3",
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "^6.1"
    },
    "autoload": {
        "psr-4": {
            "BrianFaust\\Braintree\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BrianFaust\\Tests\\Braintree\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "BrianFaust\\Braintree\\DebugbarServiceProvider"
            ],
            "aliases": {
                "Braintree": "BrianFaust\\Braintree\\Facades\\Braintree"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
